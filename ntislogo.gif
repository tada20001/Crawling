		// 팝업 오픈
		function openMainPopUpWin(url, w, h, popName){
			cw = w;
			ch = h;
			
			sw = screen.availWidth;
			sh = screen.availHeight;
			
			px = (sw-cw)/2;
			py = (sh-ch)/2;
			
			window.open(url, popName, "scrollbars=yes, status=no, menu=no, resizable=no, left="+px+", top="+py+", width="+cw+", height="+ch);
		}
		
		// 인력정보 상세보기
		function hurimsDetainInfo(hrstKoi){
			if(hrstKoi == null || hrstKoi.trim() == ""){
				alert("인력정보가 부족하여 상세 조회를 할 수 없습니다.");
				return false;
			}

			var url = "https://www.ntis.go.kr/hurims/detail/selectDetailInfo.do?hrstKOI=" + hrstKoi;
			url += "&apiGb=true&pjtId="+'1711019127';
			url += "&returnURI=" + '';

			window.location.href = url;
		}
		
		// 세부과제의 위탁과제 및 공동연구정보 목록 보기
		function viewCmCoList(pjtId){
			$("#tr" + pjtId).attr("style","display:");

			$("#" + pjtId + "close").attr("style","display:");
			$("#" + pjtId + "more").attr("style","display:none");
		}
		
		// 세부과제의 위탁과제 및 공동연구정보 목록 닫기
		function closeCmCoList(pjtId){
			$("#tr" + pjtId).attr("style","display:none");

			$("#" + pjtId + "close").attr("style","display:none");
			$("#" + pjtId + "more").attr("style","display:");
		}
		
