	function Lpad(str, num, chr) {   
	    if (! str || str.length >= num) {
	        return str;
	    }
	
	    var max = num - str.length;
	    for (var i = 0; i < max; i++) {
	        str = chr + str;
	    }
	
	    return str;
	}


